{
  "name": "<%= name %>",
  "version": "0.0.1",
  "description": "<%= description %>",
  "keywords": [],
  "homepage": "<%= website %>",
  "bugs": "",
  "main": "<%= umdFile %>",
  "jsnext:main": "<%= esFile %>",
  "author": "<%= author %>",
  "license": "MIT",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register <%= entryPath %>/**/*.spec.js",
    "build": "cross-env NODE_ENV=production rollup -c",
    "prebuild": "npm-run-all lint test",
    "watch": "cross-env NODE_ENV=development rollup -c -w",
    "browse": "browser-sync start --server --directory --files '<%= umdFile %>' --no-notify --cors",
    "start": "npm-run-all --parallel watch browse",
    "lint": "eslint <%= entryPath %>/**/*.{js,jsx}",
    "flow": "flow status --color always --strip-root",
    "sourcemap": "npm-run-all --parallel sourcemap:es sourcemap:umd",
    "sourcemap:es": "sorcery -i <%= esFile %>",
    "sourcemap:umd": "sorcery -i <%= umdFile %>"
  },
  "dependencies": {
  },
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-preset-airbnb": "^2.1.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-standalone": "^6.18.0",
    "browser-sync": "^2.17.3",
    "chai": "^3.5.0",
    "cross-env": "^3.1.2",
    "enzyme": "^2.4.1",
    "eslint": "^3.7.1",
    "eslint-plugin-flowtype": "^2.20.0",
    "eslint-plugin-react": "^6.4.1",
    "flow-bin": "^0.37.0",
    "mocha": "^3.1.0",
    "npm-run-all": "^4.0.0",
    "react-addons-test-utils": "^15.3.2",
    "rollup": "^0.40.0",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-watch": "^3.1.0",
    "sinon": "^1.17.6",
    "sorcery": "^0.10.0",
    "uglify-js": "^2.7.4"
  }
}
